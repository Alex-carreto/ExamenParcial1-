//imprime 10 números consecutivos, comenzando desde cualquier número
#include <stdio.h>

int main()
 {
 	int inicial;
 	
 	printf("Ingrese el numero inicial:");
 	scanf("%d", &inicial);
 	
 	int i=inicial;
 	int final = inicial+9;
 	
 	while (i<=final)
 	{
 		printf("%d\n",i);
 		i++;
 	}
 	printf("\n");
 	
 	return 0;
 	
}
