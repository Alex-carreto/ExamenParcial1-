//Obtiene las tablas de multiplicar de un n√∫mero hasta el multiplo que tu quieras
#include <stdio.h>
#include <math.h>

int main ()
{
	int x;
	int y;
	printf("Digite el numero que quieras sus tablas de multiplicar:");
	scanf("%d",&x);
	printf ("Digite hasta que numero quieres sus multiplos:");
	scanf("%d",&y);
	printf ("\n");
	
	printf("Tablas de multiplicar del %d\n", x);
		for (int i=1; i<=y; i++)
		{
			printf("%d x %d = %d\n", x, i, x*i);
		}
	return 0;
}
