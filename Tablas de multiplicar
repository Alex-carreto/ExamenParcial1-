//Obtiene las tablas de multiplicar de un n√∫mero hasta el multiplo que tu quieras
#include <stdio.h>
int main ()
{
	int numero;
	int y;
	
	printf("Ingrese un numero:");
	scanf("%d", &numero);
	printf ("Digite hasta que numero quieres sus multiplos:");
	scanf("%d",&y);
	printf ("\n");
	
	printf("Tabla de multiplicar de %d:\n", numero);
	int i =1;
	while (i<=y)
	{
		printf("%d x %d = %d\n", numero, i, numero *i);
		i++;
	}
	return 0;
}
